#include<stdio.h>
int main()
{
    int a,b,c,d,rev,n,m,pal;
    printf("enter a 4 digit positive integer");
    scanf("%d",&n);
    m=n;
    a=n%10;
    n=n/10;
    b=n%10;
    n=n/10;
    c=n%10;
    n=n/10;
    d=n%10;
    n=n/10;
    pal=a*1000+b*100+c*10+d;
    
     pal==m? printf("palindrome",pal): printf("not",pal);
    
    return 0;
}
