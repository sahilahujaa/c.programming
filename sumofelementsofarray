#include <stdio.h>

#include<stdlib.h>

#include <string.h>


int main() {

int i,n,s=0;
//take input of n before declaring array;
scanf("%d\n",&n);
//dynamic memory allocation/
int *arr = (int*)malloc(n* sizeof(int));

//input elements//
 for (i=0;i<n;i++){
scanf("%d",&arr[i]);
 }
 //sum//
for (i=0;i<n;i++){ s=s+arr[i];}

printf("%d",s);

free(arr); 
return 0;

}
